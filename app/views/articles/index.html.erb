<h1>Quotes</h1>


<%= form_with(url: articles_path, method: :get, data: { controller: "search-form" }) do |form| %>
  <%= form.label :Query %>
  <%= form.text_field :query, placeholder: "Search for a quote", data: { action: "input->search-form#search" } %>
<% end %>

<div>
    <%= render "articles", articles: @articles %>
</div>

